set -e
cd $(dirname $0)

order=(
	home
	sazanami
	packages
	nvtop
	intel-undervolt
	pipewire-pulse
	wpa
	bright
	nvidia-nvfbc
	issue
	optimize
	microcode
	#grapejuice
)

for i in ${order[@]}
do
	echo $i: BEGIN
	./$i/install
	echo $i: END
done

echo REBOOT FOR CHANGES

#!/usr/bin/env bash
##################################################
# Voidy, by Jeffrey Skinner.                     #
# Voidy is personalized specifically for myself, #
# however feel free to use it as you'd like.     #
##################################################

set -e
cd $(dirname $0)

order=(
	sazanami

	packages

	home

	intel-undervolt
	
	steam

	#nvidia
	nvtop
	nvdrm
	nvidia-nvfbc

	ytfetch
	pipewire-pulse
	wpa
	bright
	issue
	grub-modifications
	microcode

	bspwm
	sxhkd
)

for i in ${order[@]}
do
	echo $i: BEGIN
	./$i/install
	echo $i: END
done

echo REBOOT FOR CHANGES
